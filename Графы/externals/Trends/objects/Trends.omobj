			<do access-modifier="private" name="LayoutUpdate" display-name="LayoutUpdate" ver="5" type="void" type-id="cfbbcafa-e13e-4354-a33a-6e57b818e4da">
				<body kind="om">
					<![CDATA[//DebugTool_1.Log("WS.RS.LayoutUpdate");

splitterWidth: double = ShowLegend ? 3 : 0;
Splitter.SizeLimits.MinHeight = splitterWidth;
Splitter.SizeLimits.MinHeight = splitterWidth;

legendHeight:double = 0;
if (ShowLegend)
{
	if (Legend.bAutosize)
	{
		lines: int4 = Legend.Table_1.GetRowCount();
		if (lines < 1)
			lines = 1;	
		else if (MaxLineLegendShow > 0 && lines > MaxLineLegendShow)
			lines = MaxLineLegendShow;
		legendHeight = lines * Legend.Table_1.RowHeight + Legend.Table_1.HeaderHeight + 20;
		Legend.legendHeight = legendHeight;
	}
	else
	{
		legendHeight = Legend.legendHeight;
	}
}

if (Legend.Visible != ShowLegend)	Legend.Visible = ShowLegend;
if (Legend.LayoutWeight_1 != legendHeight) Legend.LayoutWeight_1 = legendHeight;

vf: double = WS.Height - Legend.LayoutWeight_1 - splitterWidth;
if (vf < 0) vf = 0;
if (Core.ActiveMode == unit.Const.amOPERATIVE)
{
	if (Chart_Operative.LayoutWeight_1 != vf) Chart_Operative.LayoutWeight_1 = vf;
	if (Chart_History.LayoutWeight_1 != 0) Chart_History.LayoutWeight_1 = 0;
}
else
{
	if (Chart_Operative.LayoutWeight_1 != 0) Chart_Operative.LayoutWeight_1 = 0;
	if (Chart_History.LayoutWeight_1 != vf) Chart_History.LayoutWeight_1 = vf;
}]]>
				</body>
			</do>

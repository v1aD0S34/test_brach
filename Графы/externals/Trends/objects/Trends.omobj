		<do access-modifier="private" name="SaveSettings" display-name="SaveSettings" ver="5" type="void" type-id="cfbbcafa-e13e-4354-a33a-6e57b818e4da">
			<param access-modifier="private" name="folder" display-name="" uuid="f0dda406-69e2-4a06-bba1-00f93435adfb" base-type="string" base-type-id="76403785-f3d5-41a7-9eb6-d19d2aa2d95d" ver="4"/>
			<body kind="javascript">
				<![CDATA[var filePath = FileManager.CombinePath(folder, "settings.json");
//log("Сохранение настроек в файл "  + filePath);
try
{
	var settings = {};
	
	settings.ToolBar = {
		"btnModeVisible": ToolBar.btnOperMode.IsVisible.Value,
		"btnOpenVisible": ToolBar.btnOpen.IsVisible.Value,
		"btnSaveVisible": ToolBar.btnSave.IsVisible.Value,
		"btnExportVisible": ToolBar.btnExport.IsVisible.Value,
		"btnPrintVisible": ToolBar.btnPrint.IsVisible.Value,
		"btnScreenshotVisible": ToolBar.btnScreenshot.Accessed.Value,
		"btnTreeVisible": ToolBar.btnTreeVisible.IsVisible.Value,
		"btnMiniTrendsVisible": ToolBar.btnMiniTrends.IsVisible.Value,
		"btnShowLegendVisible": ToolBar.btnShowLegend.IsVisible.Value,
		"btnIndiZoomVisible": ToolBar.btnIndiZoom.IsVisible.Value,
		"btnAutoRangeVisible": ToolBar.btnAutoRange.IsVisible.Value,
		"btnChartPointsVisible": ToolBar.btnChartPoints.IsVisible.Value,
		"btnReperVisible": ToolBar.btnReperAdd.IsVisible.Value,
		"btnHandVisible": ToolBar.btnHand.IsVisible.Value,
		"btnZoomVisible": ToolBar.btnZoom.IsVisible.Value,
		"btnHomeVisible": ToolBar.btnHome.IsVisible.Value,
		"btnZoomInOutVisible": ToolBar.btnZoomIn.IsVisible.Value,
		"btnXYZoomVisible": ToolBar.btnXZoom.IsVisible.Value,
		"btnPauseVisible": ToolBar.btnPause.Accessed.Value,
		"btnPrevNextVisible": ToolBar.RecHistory.btnPrevPeriod.Accessed.Value,
		"btnHistRequestVisible": ToolBar.RecHistory.btnRequest.Accessed.Value,
		"cbxHistInterval": ToolBar.RecHistory.EditAccessed
	};
	
	settings.ItemsTree = {
		"Width": WS.ItemsTree.treeWidth
	};
	
	settings.Print = {
		"EnableTitlePrint": Core.EnableTitlePrint,
		"TitlePrint": Core.TitlePrint
	}

	settings.ColorPalette = [];
	var i = 0;
	while (i < Core.private.ColorPalette.Length)
	{
		settings.ColorPalette.push("0x" + Core.private.ColorPalette[i].toString(16));
		i += 1;
	}
	
	if (Core.private.CustomColors != "")
		settings.CustomColors = JSON.parse(Core.private.CustomColors);
	
	var json = JSON.stringify(settings, null, 2);
	FileManager.WriteTextFile(filePath, json);
}
catch(error)
{
	log("Не удалось сохранить настройки в "  + filePath + "\n" + error.message);
}]]>
			</body>
		</do>
		<do access-modifier="private" name="LoadSettings" display-name="LoadSettings" ver="5" type="bool" type-id="7c9fe1ab-1ee2-42d8-939f-94ce3e89fca8">
			<param access-modifier="private" name="folder" display-name="" uuid="59902175-4116-499e-96cb-38cc96535af2" base-type="string" base-type-id="76403785-f3d5-41a7-9eb6-d19d2aa2d95d" ver="4"/>
			<body kind="javascript">
				<![CDATA[var filePath = FileManager.CombinePath(folder, "settings.json");
if (FileManager.Exists(filePath))
{
	//log("Загрузка настроек из файла "  + filePath);
	try
	{
		var json = FileManager.ReadTextFile(filePath);
		var settings = JSON.parse(json);
	
		if (settings.ToolBar != undefined)
		{																								
			ToolBar.btnOperMode.IsVisible = settings.ToolBar.btnModeVisible;			// Прямое управление видимостью
			ToolBar.btnHistMode.IsVisible = settings.ToolBar.btnModeVisible;							
			ToolBar.btnChartPoints.IsVisible = settings.ToolBar.btnChartPointsVisible;
			ToolBar.btnHand.IsVisible = settings.ToolBar.btnHandVisible;
			ToolBar.btnZoom.IsVisible = settings.ToolBar.btnZoomVisible;
			ToolBar.btnReperAdd.IsVisible = settings.ToolBar.btnReperVisible || settings.ToolBar.btnReperAddVisible;
			ToolBar.btnReperDel.IsVisible = settings.ToolBar.btnReperVisible || settings.ToolBar.btnReperDelVisible;
			ToolBar.btnReperDelDrop.IsVisible = settings.ToolBar.btnReperVisible || settings.ToolBar.btnReperDelVisible;
			ToolBar.btnIndiZoom.IsVisible = settings.ToolBar.btnIndiZoomVisible;
			ToolBar.btnAutoRange.IsVisible = settings.ToolBar.btnAutoRangeVisible;
			ToolBar.btnTreeVisible.IsVisible = settings.ToolBar.btnTreeVisible;
			ToolBar.btnMiniTrends.IsVisible = settings.ToolBar.btnMiniTrendsVisible;
			ToolBar.btnOpen.IsVisible = settings.ToolBar.btnOpenVisible;
			ToolBar.btnOpenDrop.IsVisible = settings.ToolBar.btnOpenVisible;
			ToolBar.btnSave.IsVisible = settings.ToolBar.btnSaveVisible;
			ToolBar.btnSaveDrop.IsVisible = settings.ToolBar.btnSaveVisible;
			ToolBar.btnPrint.IsVisible = settings.ToolBar.btnPrintVisible;
			ToolBar.btnScreenshot.Accessed = settings.ToolBar.btnScreenshotVisible != false;
			ToolBar.btnExport.IsVisible = settings.ToolBar.btnExportVisible || settings.ToolBar.btnsettingsVisible;
			ToolBar.btnHome.IsVisible = settings.ToolBar.btnHomeVisible;
			ToolBar.btnZoomIn.IsVisible = settings.ToolBar.btnZoomInOutVisible;
			ToolBar.btnZoomOut.IsVisible = settings.ToolBar.btnZoomInOutVisible;
			ToolBar.btnShowLegend.IsVisible = settings.ToolBar.btnShowLegendVisible || settings.ToolBar.btnZoomLegendVisible;
			ToolBar.btnXZoom.IsVisible = settings.ToolBar.btnXYZoomVisible;
			ToolBar.btnYZoom.IsVisible = settings.ToolBar.btnXYZoomVisible;
			ToolBar.btnPause.Accessed = settings.ToolBar.btnPauseVisible;	// Управление видимостью через флаг доступности кнопки, если существуют доп.условия
			ToolBar.RecHistory.btnPrevPeriod.Accessed = settings.ToolBar.btnPrevNextVisible;
			ToolBar.RecHistory.btnNextPeriod.Accessed = settings.ToolBar.btnPrevNextVisible;
			ToolBar.RecHistory.btnRequest.Accessed = settings.ToolBar.btnHistRequestVisible;
			ToolBar.RecHistory.EditAccessed = settings.ToolBar.cbxHistInterval;
			ToolBar.Update();
		}
	
		var itemsTree = settings.ItemsTree;
		if (itemsTree != undefined)
		{
			WS.ItemsTree.treeWidth = itemsTree.Width;
		}
		
		var settingsPrint = settings.Print;
		if (settingsPrint != undefined)
		{
			Core.EnableTitlePrint = settingsPrint.EnableTitlePrint;
			Core.TitlePrint = settingsPrint.TitlePrint
		}

		var colorPalette = settings.ColorPalette;
		if (colorPalette != undefined)
		{				
			var i = 0;
			while (i < Core.private.ColorPalette.Length)
			{
				if (colorPalette[i] != null)
					Core.private.ColorPalette[i] = parseInt(colorPalette[i], 16);
				i += 1;
			}
		}
	
		var customColors = settings.CustomColors;
		if (customColors != undefined)
		{				
			Core.private.CustomColors = JSON.stringify(customColors, 0, 2);
		}
	}
	catch(error)
	{
		log("Не удалось загрузить настройки из "  + filePath + "\n" + err.message);
		return false;
	}
}
else
{
	ToolBar.btnPause.Accessed = true;
	ToolBar.RecHistory.btnPrevPeriod.Accessed = true;
	ToolBar.RecHistory.btnNextPeriod.Accessed = true;
	ToolBar.RecHistory.btnRequest.Accessed = true;
	ToolBar.RecHistory.EditAccessed = true;
}

return true;]]>
			</body>
		</do>

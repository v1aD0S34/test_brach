	<do access-modifier="private" name="AddControlLine" display-name="Добавить реперную линию" ver="5" type="void" type-id="cfbbcafa-e13e-4354-a33a-6e57b818e4da">
		<body kind="om">
			<![CDATA[
if (Core.ActiveMode == unit.Const.amOPERATIVE)
{
	lb:uint8 = Variant.ToUint8(WS.RS.Chart_Operative.Chart.Axis_X.Min, 0);		// Левая граница отображаемого интервала
	rb:uint8 = Variant.ToUint8(WS.RS.Chart_Operative.Chart.Axis_X.Max, 0);		// Правая граница отображаемого интервала
	time:uint8 = TypeConvert.ToUint8(lb + (rb - lb) * 0.5);
	WS.RS.Chart_Operative.AddControlLineAtTime(time);
}
else
{
	lb:uint8 = Variant.ToUint8(WS.RS.Chart_History.Chart.Axis_X.Min, 0);		// Левая граница отображаемого интервала
	rb:uint8 = Variant.ToUint8(WS.RS.Chart_History.Chart.Axis_X.Max, 0);		// Правая граница отображаемого интервала
	time:uint8 = TypeConvert.ToUint8(lb + (rb - lb) * 0.5);
	WS.RS.Chart_History.AddControlLineAtTime(time);
}
]]>
		</body>
	</do>

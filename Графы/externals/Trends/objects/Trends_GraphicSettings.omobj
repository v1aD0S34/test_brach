		<do-on access-modifier="private" name="Handler_1" display-name="Handler_1" ver="5" event="ButtonPressed">
			<body kind="om">
				<![CDATA[if (private.changes == true)			// Если изменения были
{
	tag:string = pTrends.private.NowParams.Lines[private.indexItem].Tag;	// определяем тэг редактируемого графика

	pTrends.private.NowParams.Lines[private.indexItem].WidthGood = TypeConvert.ToFloat(editWidthGood.Value);
	pTrends.private.NowParams.Lines[private.indexItem].WidthBad = TypeConvert.ToFloat(editWidthBad.Value);
	if (cbStyleGood.SelectedIndex >= 0)
		pTrends.private.NowParams.Lines[private.indexItem].StyleGood = pTrends.private.LineStyles.Values[cbStyleGood.SelectedIndex];
	if (cbStyleBad.SelectedIndex >= 0)
		pTrends.private.NowParams.Lines[private.indexItem].StyleBad = pTrends.private.LineStyles.Values[cbStyleBad.SelectedIndex];
	if (MarkerComboBox.SelectedIndex >= 0)
	{
		pTrends.private.NowParams.Lines[private.indexItem].PointSymbol = pTrends.private.Markers.Symbols[MarkerComboBox.SelectedIndex];
		pTrends.private.NowParams.Lines[private.indexItem].PointSymbolCode = pTrends.private.Markers.Codes[MarkerComboBox.SelectedIndex];
	}

	pTrends.private.NowParams.Lines[private.indexItem].bSyncroColorMarkerLabels = SyncroColorMarkerLabels.State;
	pTrends.private.NowParams.Lines[private.indexItem].ColorMain = LineColorRect.BrushColor;
	if (pTrends.SynchronizeColor)	
		pTrends.private.NowParams.Axis[private.indexItem].ColorY = LineColorRect.BrushColor;
	else	
		pTrends.private.NowParams.Axis[private.indexItem].ColorY = AxisColorRect.BrushColor;			// меняем цвет индивидуальной оси

	pTrends.private.NowParams.Lines[private.indexItem].bUseColorManual = chbUseColorManual.State;
	pTrends.private.NowParams.Lines[private.indexItem].ColorManual = ManualColorRect.BrushColor;
	pTrends.private.NowParams.Lines[private.indexItem].StringTagYValue = editYValue.Value;	
	pTrends.private.Lines.Update();

	if (pTrends.private.LegendIsUse)
		pTrends.pLegend.Update();		// обновляем панель легенды

	if (editLowRangeIndivid.bChanged || editHighRangeIndivid.bChanged)
	{
		axisIndex:int4 = pTrends.GlobalRanges ? 0 : pTrends.private.NowParams.Lines[private.indexItem].numAxis;
		pTrends.pChart_Oper_1.SetYRange(axisIndex, editLowRangeIndivid.Value, editHighRangeIndivid.Value);
		pTrends.pChart_Hist_1.SetYRange(axisIndex, editLowRangeIndivid.Value, editHighRangeIndivid.Value);
	}
}
pTrends.OnSaveSettings.Invoke();
Close();]]>
			</body>

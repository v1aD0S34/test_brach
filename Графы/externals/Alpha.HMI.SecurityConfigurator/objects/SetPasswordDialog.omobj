	<do access-modifier="private" name="CopyRigths" display-name="CopyRigths" ver="5" type="void" type-id="cfbbcafa-e13e-4354-a33a-6e57b818e4da">
		<body kind="javascript">
			<![CDATA[try {
	var rightsList = JSON.parse(User.GetEffectiveRights("Alpha.Security")).data;
} catch {
	return;
}
for (var i = 0; i < rightsList.length; i++){
	let currentRight = rightsList[i];
	let TokenType = (rightsList[i].tokenType == "bool")?(1):(0);
	ProxyUser.AddRight(currentRight.appName, currentRight.tokenName, TokenType, currentRight.tokenValueAllow, currentRight.tokenValueDeny);
}
]]>
		</body>
	</do>
	<do access-modifier="private" name="CheckPassword" display-name="CheckPassword" ver="5" type="void" type-id="cfbbcafa-e13e-4354-a33a-6e57b818e4da">
		<body kind="om">
			<![CDATA[	code:uint2 = ProxyUser.SetPassword(NewPassword.Editor.Text);
	if (NewPassword.Editor.Text == "" ) code |= 1<<9;
	NewPassword.Error = GetPasswordErrorDescription(code);]]>
		</body>
	</do>
	<do access-modifier="private" name="GetPasswordErrorDescription" display-name="GetPasswordErrorDescription" ver="5" type="string" type-id="76403785-f3d5-41a7-9eb6-d19d2aa2d95d" form-by-id="true">
		<body kind="om">
			<![CDATA[error: string = "";

	if (Math.TestBit(ErrorCode, 1))
	{
		if (error != "") error += "\n";
		error += "Длина пароля не соответствует требуемой";
	}
	if (Math.TestBit(ErrorCode, 2))
	{
		if (error != "") error += "\n";
		error += "В пароле не присутствуют цифры";
	}
	if (Math.TestBit(ErrorCode, 3))
	{
		if (error != "") error += "\n";
		error += "В пароле не присутствуют символы верхнего регистра";
	}
	if (Math.TestBit(ErrorCode, 4))
	{
		if (error != "") error += "\n";
		error += "В пароле не присутствуют символы нижнего регистра";
	}
	if (Math.TestBit(ErrorCode, 5))
	{
		if (error != "") error += "\n";
		error += "В пароле не присутствуют специальные символы";
	}
	if (Math.TestBit(ErrorCode, 6))
	{
		if (error != "") error += "\n";
		error += "Ошибка при получении прав на сложность пароля для пользователя";
	}
	if (Math.TestBit(ErrorCode, 7))
	{
		if (error != "") error += "\n";
		error += "Пароль должен отличаться от предыдущих паролей";
	}
	
	if (Math.TestBit(ErrorCode, 8))
	{
		if (error != "") error += "\n";
		error += "Пароль должен содержать пробелы";
	}
	
		if (Math.TestBit(ErrorCode, 9))
	{
		if (error != "") error += "\n";
		error += "Пароль не может быть пустым";
	}
	
	if (ErrorCode > 0 && error == "") {
		error = "Не удалось установить пароль (код ошибки: " + String.ToString(ErrorCode) + ")";
	}
	
return error;
]]>
		</body>
		<param access-modifier="private" name="ErrorCode" display-name="" uuid="b61ca9cd-d7f7-410f-8446-51e32313fcf0" base-type="uint2" base-type-id="ae923ad8-8b75-4813-8121-36efe3cc8c12" ver="3"/>
	</do>

		<do-on access-modifier="private" name="Handler_1" display-name="Handler_1" ver="5" event="CurrentUserChanged">
			<body kind="om">
				<![CDATA[CurrentUserRightsChanged();
 //для деблокировки команд на случай если не отработают обработчики событий
if (GuestMode){
	BasicControlCommands.GoTo.doUnblockingControll();
}]]>
			</body>
		</do-on>
		<do access-modifier="private" name="CurrentUserRightsChanged" display-name="CurrentUserRightsChanged" ver="5" type="void" type-id="cfbbcafa-e13e-4354-a33a-6e57b818e4da">
			<body kind="om">
				<![CDATA[StatusBar.CurrentUser.Text = CurrentUserDisplayName;

limitationOfRights: string = "";
if (SecurityContext.GuestMode)
	limitationOfRights = "Необходима авторизация пользователя";
else	if (!ViewConfiguration.Value)
	limitationOfRights = "У пользователя отсутствуют права для просмотра конфигурации";
else if (!ConfigurationAccess.Value)
	limitationOfRights = "Доступен только просмотр, изменение конфигурации запрещено";
else limitationOfRights = "";
StatusBar.CurentUserRights.Text = limitationOfRights;

// отображаем панель инструментов и фрейм
Toolbar.Visible = ViewConfiguration.Value;
Frame.Visible = ViewConfiguration.Value;

// открываем форму, если она еще не была открыта
if (!GuestMode && ViewConfiguration.Value && BasicControlCommands.GoTo.frameCode != BasicControlCommands.GoTo.UserList){
	Frame.OpenFrame(BasicControlCommands.GoTo.UserList);
}]]>
			</body>
		</do>
		<do-on access-modifier="private" name="Handler_2" display-name="Handler_2" ver="5" event="ConnectedChanged">
			<body kind="om">
				<![CDATA[if (!connected){
	BasicControlCommands.GoTo.doUnblockingControll();
	StatusBar.CurentUserRights.Text = ConnectionError;
	Toolbar.Visible = connected;
	Frame.Visible = connected;
} else {
	CurrentUserRightsChanged();
}]]>
			</body>
		</do-on>
	</object>

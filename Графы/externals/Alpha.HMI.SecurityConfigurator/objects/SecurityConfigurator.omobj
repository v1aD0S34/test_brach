		<do access-modifier="private" name="FramesManagement" display-name="FramesManagement" ver="5" type="void" type-id="cfbbcafa-e13e-4354-a33a-6e57b818e4da">
			<body kind="om">
				<![CDATA[if (GoTo.CanExecute){
	if (frameCode  == GoTo.CloseMainForm){
		here.Close();
	} else if (frameCode  == GoTo.AppList){
		GoTo.sendData = "";
		Toolbar.ToolbarMode = GoTo.AppList;
		Action_AppList.Invoke();
	} else if (frameCode == GoTo.GroupList){
		GoTo.sendData = "";
		Toolbar.ToolbarMode = GoTo.GroupList;
		Action_GroupList.Invoke();
	} else if (frameCode == GoTo.UserList){
		GoTo.sendData = "";
		Toolbar.ToolbarMode = GoTo.UserList;
		Action_UsersList.Invoke();
	} else if (frameCode == GoTo.AppEdit){
		Toolbar.ToolbarMode = GoTo.AppEdit;
		Action_AppEdit.Invoke();
	} else if (frameCode == GoTo.RoleEditor){
		Toolbar.ToolbarMode = GoTo.RoleEditor;
		Action_AppRoles.Invoke();
	} else if (frameCode == GoTo.GroupEdit){
		Toolbar.ToolbarMode = GoTo.GroupEdit;
		Action_GroupEdit.Invoke();
	} else if (frameCode == GoTo.UserEdit){
		Toolbar.ToolbarMode = GoTo.UserEdit;
		Action_EditUser.Invoke();
	} else if (frameCode == GoTo.UserCreate){
		Toolbar.ToolbarMode = GoTo.UserCreate;
		Action_CreateUser.Invoke();
	}
}]]>
			</body>
			<param access-modifier="private" name="frameCode" display-name="" uuid="2ae4c948-62df-4334-ad23-a8066a7d31d6" base-type="uint1" base-type-id="f73099bc-ee13-42cf-82bb-87368b826707" ver="4"/>
		</do>

	<do access-modifier="private" name="SetToTable" display-name="SetToTable" ver="5" type="void" type-id="cfbbcafa-e13e-4354-a33a-6e57b818e4da">
		<body kind="javascript">
			<![CDATA[//при изменении списка прав перестраиваем модель таблицы

if (RightsData.GetColumnCount() == 0) RightsData.Init();
RightsData.Clear();

try {
	var rightsData = JSON.parse(RightsJson).data;
	currentRightsListJson = RightsJson;
} catch (error) {
	var rightsData = [];
}

try { 
	var effectiveRightsData = JSON.parse(EffectiveRightsJson).data;
} catch (error) {
	var effectiveRightsData = [];
}
//формируем список приложений
var appList = [];	
if (effectiveRightsData.length != 0) {																			//Получаем список имен приложений
	for (var i = 0; i < effectiveRightsData.length; i++) {
		if ((appList.indexOf(effectiveRightsData[i].appName) === -1) ){
			appList.push(effectiveRightsData[i].appName);
		}
	}
	appList = appList.sort();
	effectiveRightsData = effectiveRightsData.sort((a, b) => a.tokenName.toString().toLowerCase() > b.tokenName.toString().toLowerCase() ? 1 : -1);
}
//заполняем модель таблицы с группировкой прав по приложениям
for (var i = 0; i<appList.length; i++){			//цикл перебора приложений
	var currentRow = RightsData.GetRowCount();
	var currentApp = appList[i];
	RightsData.AddRow();
	RightsData.SetCellData(currentRow,0, " ");	
	RightsData.SetCellData(currentRow,10, " "); //отметка строки приложения для декоратора 
	RightsData.SetCellData(currentRow,1, appList[i]);
	
	var currentAppRights = [];
	for (var j = 0; j < effectiveRightsData.length; j++){		//выбрать права для данного приложения в одном экземляре
		if ((effectiveRightsData[j].appName == currentApp) && (appList.indexOf(effectiveRightsData[j].tokenName) === -1)){
			currentAppRights.push(effectiveRightsData[j]);
		}
	}
	//заполняем модель таблицы данными прав по каждому приложению
	for (var a =0; a < currentAppRights.length; a++){
		var lastRow = RightsData.GetRowCount();
		RightsData.AddRow();
		RightsData.SetCellData(lastRow, 0, currentAppRights[a].tokenType);
		RightsData.SetCellData(lastRow, 1, currentAppRights[a].tokenName);
		for (var n = 0; n < rightsData.length; n++){		//если есть у аккаунта есть свои права, то добавляем в таблицу их значения
			if (rightsData[n].appName == currentAppRights[a].appName && rightsData[n].tokenType ==  currentAppRights[a].tokenType 
			&& rightsData[n].tokenName == currentAppRights[a].tokenName){
				RightsData.SetCellData(lastRow, 2, rightsData[n].tokenValueAllow);
				RightsData.SetCellData(lastRow, 3, rightsData[n].tokenValueDeny);
				//определяем значение для визуального столбца текущих прав
				RightsData.SetCellData(lastRow, 4,TokensDefinition.DetermineRightsValue(rightsData[n].appName, rightsData[n].tokenType, rightsData[n].tokenName, rightsData[n].tokenValueAllow, rightsData[n].tokenValueDeny));
			}
		}
		RightsData.SetCellData(lastRow, 5, currentAppRights[a].tokenValueAllow);
		RightsData.SetCellData(lastRow, 6, currentAppRights[a].tokenValueDeny);
		RightsData.SetCellData(lastRow, 7, TokensDefinition.DetermineRightsValue(currentAppRights[a].appName, currentAppRights[a].tokenType, currentAppRights[a].tokenName, currentAppRights[a].tokenValueAllow, currentAppRights[a].tokenValueDeny));
		RightsData.SetCellData(lastRow, 8, currentAppRights[a].tokenDescription);
		RightsData.SetCellData(lastRow, 9, currentAppRights[a].appName);
		RightsData.SetCellData(lastRow, 10, TokensDefinition.GetTypeTitle(currentAppRights[a].appName, currentAppRights[a].tokenType, currentAppRights[a].tokenName));
	}
}

//переход к выделенной ячейке
if (RightsTable.selectedRow >= 0 && RightsTable.selectedRow <= RightsTable.GetRowCount()){
	RightsTable.ScrollTo(RightsTable.selectedRow,2,3);
} else {
	RightsTable.ScrollToTop();
}]]>

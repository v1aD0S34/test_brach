		<object access-modifier="private" name="Complete" display-name="Complete" uuid="97792044-5140-4ad7-85e1-3f342685ae95" base-type="Action" base-type-id="a9ee9770-1c4a-44c9-b815-157d9fc2ab95" ver="4">
			<designed target="Enabled" value="true" ver="4"/>
			<designed target="InvokeTrigger" value="false" ver="4"/>
			<do-on access-modifier="private" name="Handler_1" display-name="Handler_1" ver="5" event="Invoked">
				<body kind="om">
					<![CDATA[// если имя файла не содержит расширения, то добавим расширение из фильтра типа
fileName: string = BottomPanel.FileEdit.Text;
if (String.IndexOf(fileName, ".") == -1)
{
	fileName += BottomPanel.FileType.selectedExt;
	BottomPanel.FileEdit.Text = fileName;
}

// формируем результат и закрываем диалог
Result.FolderPath = FolderPanel.FolderEdit.Text;
Result.FileName = BottomPanel.FileEdit.Text;
Result.FilePath = Files.ToNative(Files.AbsolutePath(Files.CombinePath(FolderPanel.FolderEdit.Text, BottomPanel.FileEdit.Text)));
Result.FileType = BottomPanel.FileType.SelectedIndex;
OnComplete.Invoke();
Close();]]>
				</body>
			</do-on>
			<do-trace access-modifier="private" target="Enabled" ver="5">
				<body>
					<![CDATA[BottomPanel.FileEdit.Text != "" &&
Files.IsFile(Files.CombinePath(FolderPanel.FolderEdit.Text,
CoerseFileName(BottomPanel.FileEdit.Text, BottomPanel.FileType.selectedExt, BottomPanel.FileType.selectedExt)))]]>
				</body>
			</do-trace>
		</object>

				<![CDATA[if (oldValue != Value)
{
	oldValue = Value;	
	
	height: double = bottomPanelHeight;
	if (ShowBrowsePanel)
		height += BrowsePanel.Height;
	if (ChangeFolderAllowed)
		height += FolderPanel.Height;
	windowHeight: int4 = TypeConvert.ToInt4(height);
	
	if (WindowHeight != windowHeight)

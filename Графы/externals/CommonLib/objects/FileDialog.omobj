{
	// меняем расширение файла, если оно отсутствует или соответствует предыдущему фильтру
	i: int4 = String.LastIndexOf(fileName, ".");
	if (i == -1)
	{
		fileName += newExt;
	}
	else
	{
		if (String.EndsWith(fileName, oldExt))
			fileName = String.SubString(fileName, 0, i) + newExt;

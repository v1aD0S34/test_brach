		</do-on>
		<do-on access-modifier="private" name="Handler_3" display-name="Handler_3" ver="5" event="TextChanged">
			<body kind="javascript">
				<![CDATA[// проверяем, что введена цифра
if (Focus && /\d+/.test(Text))
{		
	var inputText = Text;
	if (inputText.length > 2)
	{
		// если введено больше символов, то оставляем последний
		inputText = inputText.slice(-1);
		Text = inputText;
	}
	else
	{
		//Debug.Log("Editor enter number " + inputText);
		var number = Number.parseInt(inputText);
		
		if (Fields.SelectedField.Value == 1)
		{
			// если введенный час больше допустимого, то сбрасываем ввод
			if (number > 23)
				Text = "";
			else Fields.Hour.Value = number;
		}
		else if (Fields.SelectedField.Value == 2)
		{
			// если введенные минуты больше допустимого, то сбрасываем ввод
			if (number > 59)
				Text = "";
			else Fields.Minute.Value = number;
		}
		else if (Fields.SelectedField.Value == 3)
		{
			// если введенные секунды больше допустимого, то сбрасываем ввод
			if (number > 59)
				Text = "";
			else Fields.Second.Value = number;
		}
	}
}]]>
			</body>
		</do-on>

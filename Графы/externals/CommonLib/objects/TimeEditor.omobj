		<do-on access-modifier="private" name="Handler_1" display-name="Handler_1" ver="5" event="ValueChanged">
			<body kind="om">
				<![CDATA[if (oldValue != Value)
{
	oldValue = Value;
	
	if	(Value)
	{
		Fields.Margins.Left = 1;
		Fields.Margins.Right = 1;
		SizeLimits.MaxWidth = 75;
	}
	else
	{
		Fields.Margins.Left = 0;
		Fields.Margins.Right = 0;
		SizeLimits.MaxWidth = 0;
	}
}]]>
			</body>
		</do-on>

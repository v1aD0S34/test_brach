		<do-on access-modifier="private" name="Handler_2" display-name="Handler_2" ver="5" event="FocusChanged">
			<body kind="om">
				<![CDATA[//Debug.Log("Editor focus changed to " + String.ToString(Focus));
if (!Focus)

	<object access-modifier="private" name="Value" display-name="Value" uuid="92c3e4b5-46fd-466a-a397-8f57a6d2d062" base-type="notifying_timestamp" base-type-id="fb54ffc6-3bfa-4c71-8ae4-fc5d62558895" ver="5">
		<do-on access-modifier="private" name="Handler_1" display-name="Handler_1" ver="5" event="ValueChanged">
			<body kind="om">
				<![CDATA[if (!Editor.Focus)
{
	// отображаем значение даты
	Fields.Hour.Value = DateTime.Hour(Value);
	Fields.Minute.Value = DateTime.Minute(Value);
	Fields.Second.Value = DateTime.Second(Value);
	//Debug.Log("TimeEditor show " + DateTime.ToString(Value));
}]]>
			</body>
		</do-on>
		<do access-modifier="private" name="Update" display-name="Update" ver="5" type="void" type-id="cfbbcafa-e13e-4354-a33a-6e57b818e4da">
			<body kind="om">
				<![CDATA[// корректируем поля
Fields.Coerse();

// формируем новое значение и уведомляем, если оно изменилось
newValue: uint8 = DateTime.Create(DateTime.Year(Value), DateTime.Month(Value), DateTime.Day(Value), Fields.Hour.Value, Fields.Minute.Value, Fields.Second.Value);
if (here.Value != newValue)
{
	here.Value = newValue;
	pValueChanged.Invoke();
}]]>
			</body>
		</do>
	</object>

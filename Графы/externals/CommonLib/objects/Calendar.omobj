		<do access-modifier="private" name="UpdateLayout" display-name="UpdateLayout" ver="5" type="void" type-id="cfbbcafa-e13e-4354-a33a-6e57b818e4da">
			<body kind="om">
				<![CDATA[i: int8 = 0;
c: int4 = 0;
x: double = 1;
y: double = 1;
while(i < Days.Length)
{
	Days[i].X = x;	
	Days[i].Y = y;

	c += 1;
	x += Days.Width + 1;
	if (c > 6)
	{
		c = 0;
		x = 0;
		y += Days.Height + 1;
	}
	
	i += 1;
}]]>
			</body>
		</do>
		<do access-modifier="private" name="Select" display-name="Select" ver="5" type="void" type-id="cfbbcafa-e13e-4354-a33a-6e57b818e4da">
			<body kind="om">
				<![CDATA[//Debug.Log("Select " + DateTime.ToString(selectedDate));

// Получим день, месяц и год из значения времени
year: uint2 = DateTime.Year(selectedDate);
month: uint1 = DateTime.Month(selectedDate);
day: uint1 = DateTime.Day(selectedDate);
SelectedDate = DateTime.Create(year, month, day, 0, 0, 0);

// Узнаем первый день месяца
firstDay: uint8 = DateTime.Create(year, month, 1, 0, 0, 0, 0);

// И его день недели
firstWeekDay: uint1 = DateTime.DayOfWeek(firstDay);	// 0-вс, 1-пн и тд

// Сдвинем нумерацию с 0 на 1, чтобы понедельник был первый день недели
firstWeekDay = TypeConvert.ToUint1(((firstWeekDay + 6) % 7) + 1);

// Покажем также последнюю неделю предыдущего месяца
if (firstWeekDay == 1)
	firstDay = DateTime.AddDays(firstDay, -7);

// Найдём первый понедельник первой недели
date: uint8 = DateTime.AddDays(firstDay, -(firstWeekDay - 1));

i: int8 = 0;
while(i < Days.Length)
{
	// установим дату
	Days[i].Date = date;
	Days[i].IsNow = date == Today.Date;
	Days[i].Selected = date == SelectedDate;
	Days[i].Label.Text = String.ToString(DateTime.Day(date));
	
	// если это день текущего месяца, то выделяем его цветом шрифта
	Days[i].Label.FontColor = (DateTime.Month(date) == month) ? theme.ForegroundColor : theme.DisabledFontColor;

	// переходим к следующему дню	
	date = DateTime.AddDays(date, 1);
	i += 1;
}]]>
			</body>

	<do-on access-modifier="private" name="Handler_2" display-name="Handler_2" ver="5" event="Opened">
		<body kind="om">
			<![CDATA[// размещаем элементы
DaySelector.UpdateLayout();

// вычисляем текущую дату
now: uint8 = DateTime.Now();
year: uint2 = DateTime.Year(now);
month: uint1 = DateTime.Month(now);
day: uint1 = DateTime.Day(now);

Today.Date = DateTime.Create(year, month, day, 0, 0, 0);
Today.Label.Text = "Сегодня: " + DateTime.ToString(Today.Date, "%d.%m.%Y");

// если дата задана некорректно, то используем текущую дату
if (Date == 0)
	Date = Today.Date;

// выбираем нужную дату
DaySelector.Select(Date);]]>
		</body>
	</do-on>

		<do access-modifier="private" name="UpdateDecorator" display-name="UpdateDecorator" ver="5" type="void" type-id="cfbbcafa-e13e-4354-a33a-6e57b818e4da">
			<body kind="javascript">
				<![CDATA[//Применение декоратора для таблицы настройки событий
var Object = {};
Object.Decorating = [];
Object.StyleDictionary = [];

for (let i = 0; i < GetRowCount(); i++)
{
	var backgroundColor = '#'+ TableModel.GetCellData(i,4).toString(16);
	var textColor = '#'+ TableModel.GetCellData(i,5).toString(16);
	var blinkBackColor = '#'+ TableModel.GetCellData(i,8).toString(16);
	var blinkTextColor = '#'+ TableModel.GetCellData(i,9).toString(16);
	var ackBackgroundColor = '#'+ TableModel.GetCellData(i,11).toString(16);
	var ackTextColor = '#'+ TableModel.GetCellData(i,12).toString(16);

	//Заполняем объект - массив Decorating
	let decorating = {};
	decorating[`decoration_rule_name`] = "rule_" + i;
	decorating[`decoration_rule`] = {'==':{'data:id':i}};
	decorating[`columnstyle`] = {};
	decorating[`columnstyle`][`example`] = "exampleStyle_" + i;
	decorating[`columnstyle`][`blinking`] = "blinkStyle_" + i;
	decorating[`columnstyle`][`ack`] = "ackStyle_" + i;
	Object.Decorating.push(decorating);
	
	//Заполняем объект - массив StyleDictionary
	let exampleStyle = {};
	exampleStyle[`style_name`] = "exampleStyle_" + i;
	exampleStyle[`style`] = {};
	exampleStyle[`style`][`background_color`] = backgroundColor;
	exampleStyle[`style`][`color`] = textColor;
	Object.StyleDictionary.push(exampleStyle);

	let blinkStyle = {};
	blinkStyle[`style_name`] = "blinkStyle_" + i;
	blinkStyle[`style`] = {};
	blinkStyle[`style`][`background_color`] = backgroundColor;
	blinkStyle[`style`][`color`] = textColor;
	var blinkingTime = "0";
	switch (TableModel.GetCellData(i,7))
	{
		case "0.5 сек":
			blinkingTime = "500";
			break;
		case "1 сек":
			blinkingTime = "1000";
			break;
		case "2 сек":
			blinkingTime = "2000";
			break;
	}
	if (blinkingTime != "0")
	{
		blinkStyle[`style`][`blinker`] = {};
		blinkStyle[`style`][`blinker`][`blinker_color`] = blinkTextColor;
		blinkStyle[`style`][`blinker`][`blinker_background_color`] = blinkBackColor;
		blinkStyle[`style`][`blinker`][`blinker_period`] = blinkingTime;
	}
	Object.StyleDictionary.push(blinkStyle);
	
	let ackStyle = {};
	ackStyle[`style_name`] = "ackStyle_" + i;
	ackStyle[`style`] = {};
	ackStyle[`style`][`background_color`] = ackBackgroundColor;
	ackStyle[`style`][`color`] = ackTextColor;
	Object.StyleDictionary.push(ackStyle);
}

Object.Formatting = [];
Object.Formatting.push({'formatting_rule_name':'example','formatting_column':'example','text':'Пример'});
Object.Formatting.push({'formatting_rule_name':'ack','formatting_column':'ack','text':'Пример'});

//log(JSON.stringify(Object, null, 2));
Decorator.JsonDecorator = JSON.stringify(Object);
]]>
			</body>
		</do>

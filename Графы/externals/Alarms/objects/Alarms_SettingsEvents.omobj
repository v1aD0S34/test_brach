		<object access-modifier="private" name="SetColumns" display-name="SetColumns" uuid="7ae6eac6-eb8d-4220-920c-d116f6df0762" base-type="Action" base-type-id="a9ee9770-1c4a-44c9-b815-157d9fc2ab95" ver="4">
			<designed target="Enabled" value="true" ver="4"/>
			<designed target="InvokeTrigger" value="false" ver="4"/>
			<do-on access-modifier="private" name="Handler_1" display-name="Handler_1" ver="5" event="Invoked">
				<body kind="javascript">
					<![CDATA[var tableColumns=[];
for (let i = 0; i < pSettingsForm.pAlarms.Main_tables_Operative.Table_Operative_mode.Columns.Length; i++)
	if (pSettingsForm.pAlarms.Main_tables_Operative.Table_Operative_mode.Columns[i].Title != "-")//cheat1
		tableColumns.push(pSettingsForm.pAlarms.Main_tables_Operative.Table_Operative_mode.Columns[i]);

var blinkingColumns = [];

var columns = JSON.parse(BlinkColumns.Value);
for(let j = 0; j < tableColumns.length; j++)
{
	var tableColumn = tableColumns[j];
	if (tableColumn != null)
	{
		for(let i = 0; i < columns.length; i++)
		{
			// в диалог выбора столбцы поступают отсортированные, поэтому идентифицируем их по заголовку
			//cheat3 использование заголовка для передачи данных нестабильно
			if (columns[i].Title == tableColumn.Title)
			{
				if (columns[i].State)
					blinkingColumns.push(tableColumn.Id);
				break;
			}
		}
	}
}

pSettingsForm.Settings.Events.BlinkingColumnsSet(JSON.stringify(blinkingColumns));]]>
				</body>
			</do-on>
		</object>
		<do access-modifier="private" name="GetColumns" display-name="GetColumns" ver="5" type="string" type-id="76403785-f3d5-41a7-9eb6-d19d2aa2d95d">
			<body kind="javascript">
				<![CDATA[var tableColumns=[];
for (let i = 0; i < pSettingsForm.pAlarms.Main_tables_Operative.Table_Operative_mode.Columns.Length; i++)
	if (pSettingsForm.pAlarms.Main_tables_Operative.Table_Operative_mode.Columns[i].Title != "-")//cheat1
		tableColumns.push(pSettingsForm.pAlarms.Main_tables_Operative.Table_Operative_mode.Columns[i]);

var blinkingColumns = JSON.parse(pSettingsForm.Settings.Events.BlinkingColumns == "" ? "[]": pSettingsForm.Settings.Events.BlinkingColumns);

var columns = [];
for(let i = 0; i < tableColumns.length; i++)
{
	var tableColumn = tableColumns[i];
	if (tableColumn != null)
	{
		var column = {};
		column.Index = tableColumn.GetIndex();
		column.Title = tableColumn.Title;
		column.State = blinkingColumns.includes(tableColumn.Id);
		columns.push(column);
	}
}

columns.sort(function (a, b)
{
  if (a.Index > b.Index)
    return 1;
  else if (a.Index < b.Index)
    return -1;
  else return 0;
});

return JSON.stringify(columns);]]>
			</body>
		</do>
	</object>

		<do-on access-modifier="private" name="Handler_1" display-name="Handler_1" ver="5" event="ButtonPressed">
			<body kind="javascript">
				<![CDATA[var filter = {};
filter['AND'] = [];

if (Severity.Filter != "") 
{
	var severityFilter = JSON.parse(Severity.Filter);
	filter['AND'].push(severityFilter);
}

if (Message.Filter != "")
{
	var messageFilter = JSON.parse(Message.Filter);
	filter['AND'].push(messageFilter);
}

if (Source.Filter != "")
{
	var sourceFilter = JSON.parse(Source.Filter);
	filter['AND'].push(sourceFilter);
}

if (Acknowledged.Filter != "") 
{
	var acknowledgedFilter = JSON.parse(Acknowledged.Filter);
	filter['AND'].push(acknowledgedFilter);
}

// Если было заполнено только одно поле фильтра - выводим это поле, иначе - массив полей.
var jsonFilter;
if (filter['AND'].length > 1)
	jsonFilter = JSON.stringify(filter);
else jsonFilter = JSON.stringify(filter['AND'][0]);

// Выполняем запрос данных с учетом фильтра
try
{
	Link_Alarms.Main_tables_Historical.SetRequestFilter(jsonFilter);
	Link_Alarms.Main_panel.private.LoadHistory();
	applyError = "";
}
catch(err)
{
	Link_Alarms.Main_tables_Historical.SetRequestFilter("");
	applyError = err.toString();
}

if (applyError == "")
{
	// запоминаем поля фильтра
	Link_Alarms.Main_panel.private.RequestFilter.severity = Severity.Editor.Text;
	Link_Alarms.Main_panel.private.RequestFilter.message = Message.Editor.Text;
	Link_Alarms.Main_panel.private.RequestFilter.source = Source.Editor.Text;
	Link_Alarms.Main_panel.private.RequestFilter.acknow = Acknowledged.Editor.GetItem(Acknowledged.Editor.SelectedIndex);
	
	Close();
}
else
	onError.Invoke();]]>
			</body>
		</do-on>

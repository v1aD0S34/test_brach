	<do access-modifier="private" name="ParseFont" display-name="ParseFont" ver="5" type="void" type-id="cfbbcafa-e13e-4354-a33a-6e57b818e4da">
		<body kind="javascript">
			<![CDATA[var fontParams = pSettingsForm.Settings.View.Font.split(",");
var fontTitleParams = pSettingsForm.Settings.View.FontTitle.split(",");

RowFontSize.Value = fontParams[1];
TitleFontSize.Value = fontTitleParams[1];

var rowFontStyle = 0;
if (fontParams[4] == 75) rowFontStyle |= 1;
if (fontParams[5] == 1) rowFontStyle |= 2;
if (fontParams[6] == 1) rowFontStyle |= 4;
RowFontStyle.Code = rowFontStyle;

var titleFontStyle = 0;
if (fontTitleParams[4] == 75) titleFontStyle |= 1;
if (fontTitleParams[5] == 1) titleFontStyle |= 2;
if (fontTitleParams[6] == 1) titleFontStyle |= 4;
TitleFontStyle.Code = titleFontStyle;]]>
		</body>
	</do>
	<do access-modifier="private" name="SetFontStyle" display-name="SetFontStyle" ver="5" type="string" type-id="76403785-f3d5-41a7-9eb6-d19d2aa2d95d">
		<body kind="javascript">
			<![CDATA[var fontParams = font.split(",");
fontParams[4] = (style & 1) == 1 ? 75 : 50; // bold
fontParams[5] = (style >> 1) & 1; // italic
fontParams[6] = (style >> 2) & 1; // underline
return fontParams.toString();]]>
		</body>

		<do access-modifier="private" name="UpdateIsVisible" display-name="UpdateIsVisible" ver="5" type="void" type-id="cfbbcafa-e13e-4354-a33a-6e57b818e4da">
			<body kind="om">
				<![CDATA[// Функция обновления состояния видимости столбцов
indexTimeGenState: int4;
indexTimeActState: int4;

i: int4 = 0;
while (i < IsVisible.Length)
{
	if (i < GetRowCount())
	{
		IsVisible[i].Visible = true && (IsVisible[i].Y >= 0) && (IsVisible[i].Y+IsVisible.Height <= ColumnsTable.Height);
		IsVisible[i].State = Variant.ToBool(TableModel.GetCellData(MapRowToModel(i), 2), false);
		IsVisible[i].Enabled = true;
		
		// Проверяем условия доступности чекбоксов для особых столбцов:
		title: string = GetCellData(i, 0);
		if (title == "Сообщение")
		{
			// Столбец "Сообщение" всегда отображается
			IsVisible[i].Enabled = false;
		}
		else if (title == "Время генерации")
		{
			indexTimeGenState = i;
		}
		else if (title == "Время срабатывания")
		{
			indexTimeActState = i;
		}
	}
	else
		IsVisible[i].Visible = false;

	i = i + 1;
}

if (IsVisible[indexTimeGenState].State && !IsVisible[indexTimeActState].State)
	IsVisible[indexTimeGenState].Enabled = false;
	
if (!IsVisible[indexTimeGenState].State && IsVisible[indexTimeActState].State)
	IsVisible[indexTimeActState].Enabled = false;
]]>
			</body>
		</do>
		<do access-modifier="private" name="UpdateTWErrors" display-name="UpdateTWErrors" ver="5" type="void" type-id="cfbbcafa-e13e-4354-a33a-6e57b818e4da">
			<body kind="om">
				<![CDATA[error: bool = false;

//Проверяем наличие ошибок в столбце ширины
i: int4 = 0;
while (i < WidthError.Length)
{
	WidthError[i].Visible = false;
	TitleError[i].Visible = false;
	
	if (i < GetRowCount())
	{
		columnWidthStr: string = Variant.ToString(GetCellData(i,3), "");
		columnWidth: int4 = String.ToInt4(columnWidthStr, -1);
		if (columnWidth == -1 || columnWidth < 0 || columnWidth > 65535)
		{
			WidthError[i].Visible = true && (WidthError[i].Y >= ColumnsTable.HeaderHeight) && (WidthError[i].Y+WidthError.Height <= ColumnsTable.Height);
			error = true;
		}
		//Проверяем текст заголовка
		titleStr: string = Variant.ToString(GetCellData(i,1), "");
		titleLen: int4 = String.Length(titleStr);
		if (titleLen < 1 || titleLen > 100)
		{
			TitleError[i].Visible = true && (TitleError[i].Y >= ColumnsTable.HeaderHeight) && (TitleError[i].Y+TitleError.Height <= ColumnsTable.Height);
			error = true;
		}
	}

	i += 1;
}

pSettingsForm.Settings.View.Error = error;]]>
			</body>
		</do>
		<do access-modifier="private" name="ShiftRow" display-name="ShiftRow" ver="5" type="void" type-id="cfbbcafa-e13e-4354-a33a-6e57b818e4da">
			<body kind="om">
				<![CDATA[//Функция сдвига строки вверх-вниз на указаное количество строк

nextRow: curRow + offset; // Индекс следующей строки в таблице
modelCurRow: int4 = MapRowToModel(curRow); // Индекс текущей строки в модели
modelNextRow: int4 = MapRowToModel(nextRow); // Индекс следующей строки в модели

// Получаем текущие значения индексов столбцов
nextRowIndex: variant = TableModel.GetCellData(modelNextRow, 4);
currentRowIndex: variant = TableModel.GetCellData(modelCurRow, 4);
//Debug.Log("Move row from " + String.ToString(currentRowIndex) + " to " + String.ToString(nextRowIndex));

// Меняем индексы местами
TableModel.SetCellData(modelNextRow, 4, currentRowIndex);
TableModel.SetCellData(modelCurRow, 4, nextRowIndex);
pSettingsForm.Settings.View.OnEdited();
UpdateIsVisible();
UpdateTWErrors();

// Выделяем перенесенную строку
curRow = nextRow;
SetSelectedRows("[" + String.ToString(curRow) + "]");	
UpdateButtonEnable();]]>
